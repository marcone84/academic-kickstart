@article{almohanna_impact_2015-1,
	title = {Impact of {Dietary} {Acculturation} on the {Food} {Habits}, {Weight}, {Blood} {Pressure}, and {Fasting} {Blood} {Glucose} {Levels} of {International} {College} {Students}.},
	volume = {63},
