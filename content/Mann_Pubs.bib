	pages = {199--213},
	file = {Attachment:/Users/marcusmann/Zotero/storage/HYTDUB7R/Weiner, Osborne, Rudolph - 2011 - An attributional analysis of reactions to poverty the political ideology of the giver and the perceive.pdf:application/pdf}
