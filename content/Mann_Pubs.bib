	file = {Attachment:/Users/marcusmann/Zotero/storage/WC96UR9V/Goldberg - 2011 - Mapping Shared Understandings Using Relational Class Analysis The Case of the Cultural Omnivore Reexamined.pdf:application/pdf}
}

